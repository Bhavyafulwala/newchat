<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/bhavyaview"
        android:layout_width="@dimen/_180sdp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/_130sdp"
        android:src="@drawable/bhavya" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/messengertext"
        android:layout_below="@+id/bhavyaview"
        android:text="MESSENGER.CO"
        android:textSize="@dimen/_20sdp"
        android:textStyle="bold"
        android:layout_marginStart="@dimen/_83sdp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/companytext"
        android:layout_below="@id/messengertext"
        android:text="FULWALA.CO"
        android:textStyle="bold"
        android:layout_marginStart="@dimen/_119sdp"
        android:layout_marginTop="@dimen/_220sdp"
        android:textSize="@dimen/_12sdp"/>








</RelativeLayout>